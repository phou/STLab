global protocol Calc(reliable role S, reliable role Cl) {
	rec Loop {
		choice at Cl {
			sum(pair) from Cl to S ;
			result(int) from S to Cl ;
			continue Loop ;
		} or {
			multiply(pair) from S to Cl;
			result(int) from S to Cl ;
			continue Loop ;
		} or {
			quit from Cl to S ;
			terminate from S to Cl ;
		}
	}
}